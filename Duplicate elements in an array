#include <stdio.h>

int main() {
    int arr[100], n, i, j;

    // Input size of array
    printf("Enter size of array: ");
    scanf("%d", &n);

    // Input elements
    printf("Enter elements of array: ");
    for (i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    printf("Duplicate elements are: ");
    int found = 0;

    // Compare each element with the rest
    for (i = 0; i < n; i++) {
        for (j = i + 1; j < n; j++) {
            if (arr[i] == arr[j]) {
                printf("%d ", arr[i]);
                found = 1;
                break; // avoid printing the same duplicate multiple times
            }
        }
    }

    if (!found) {
        printf("No duplicates found.");
    }

    return 0;
}
